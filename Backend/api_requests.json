{
  "register_user": {
    "url": "http://localhost:8080/api/v1/auth/register",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "username": "testuser",
      "email": "test@example.com",
      "password": "Test123!"
    },
    "description": "Register a new user"
  },
  
  "login_user": {
    "url": "http://localhost:8080/api/v1/auth/authenticate",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "email": "test@example.com",
      "password": "Test123!"
    },
    "description": "Authenticate and get JWT token"
  },
  
  "test_secured_endpoint": {
    "url": "http://localhost:8080/api/v1/demo-controller",
    "method": "GET",
    "headers": {
      "Authorization": "Bearer YOUR_JWT_TOKEN_HERE"
    },
    "description": "Test secured endpoint (requires authentication)"
  },
  
  "create_gig": {
    "url": "http://localhost:8080/api/v1/gigs",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer YOUR_JWT_TOKEN_HERE"
    },
    "body": {
      "gigTitel": "Summer Concert 2025",
      "gigLocation": "Eisenstadt Main Square",
      "gigDate": "2025-07-15",
      "gigDescription": "Annual summer concert featuring classical and modern pieces."
    },
    "description": "Create a new gig (requires authentication)"
  },
  
  "get_all_gigs": {
    "url": "http://localhost:8080/api/v1/gigs",
    "method": "GET",
    "description": "Get all gigs (public endpoint)"
  },
  
  "get_gig_by_id": {
    "url": "http://localhost:8080/api/v1/gigs/1",
    "method": "GET",
    "description": "Get a specific gig by ID (public endpoint)"
  },
  
  "upload_image": {
    "url": "http://localhost:8080/api/v1/images/upload",
    "method": "POST",
    "headers": {
      "Authorization": "Bearer YOUR_JWT_TOKEN_HERE"
    },
    "body": {
      "file": "[BASE64_ENCODED_IMAGE]"
    },
    "description": "Upload an image (requires authentication)"
  },
  
  "get_all_images": {
    "url": "http://localhost:8080/api/v1/images",
    "method": "GET",
    "description": "Get all images (public endpoint)"
  },
  
  "get_image_by_id": {
    "url": "http://localhost:8080/api/v1/images/1",
    "method": "GET",
    "description": "Get a specific image by ID (public endpoint)"
  }
}
